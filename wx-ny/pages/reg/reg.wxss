/* pages/reg/reg.wxss */
.reg-container{padding: 10px;}
.reg_logo{width: 321rpx;height: 150rpx;}
.center{text-align: center;margin-top: 10%;}
.reg-form{width: 80%;margin:0 5% 6%;padding:10% 5% 15%;display: block;}
.one{padding: 10rpx 20rpx;border-bottom: solid 1rpx #b5b5b5;overflow: hidden;margin: 25rpx 0;position: relative;}
.one input{width:100%;margin: 0 10rpx 0 15rpx;font-size: 30rpx;padding-left: 30rpx;padding-right:30rpx;}
.reg-left{position:absolute;left:15rpx;top:10rpx;width: 36rpx;height: 40rpx;margin-top: 0;}
.reg-right{position:absolute;right:15rpx;top:10rpx;width: 40rpx;height: 24rpx;margin-top: 15rpx;}
/* .one .input-yzm{} */
 .get-yzm{float:right;width: auto;font-size: 30rpx;color:#4ac372;position:absolute;right:0;top:10rpx;} 
.reg-btn{display:block;margin-top: 90rpx;margin-bottom: 20rpx;}
.reg-btn button{font-size:32rpx;color:#fff;display: block;width: 100%;height: 80rpx;line-height:78rpx;border:solid 1rpx #4ac372;background-color: #4ac372;border-radius: 80rpx;}
.box{overflow: hidden;padding: 0 20rpx;margin-top:-20rpx;font-size:22rpx;}
.reg-check{width: 24rpx;height: 24rpx;margin-right: 15rpx;margin-bottom:-2rpx;}
.log-btn{color: #4ac372;text-align: center;margin-bottom: 50rpx;font-size: 26rpx;}
